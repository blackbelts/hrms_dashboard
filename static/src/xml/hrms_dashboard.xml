  </xpath>
  <t t-name="HrDashboardMain">
    <xpath expr="." position="inside">
      <script src="/hrms_dashboard/static/src/js/Chart.min.js" type="text/javascript"/>
    </xpath>
    <div class="o_dashboards">
      <div class="container-fluid o_hr_dashboard">
      </div>
    </div>
  </t>
  <t t-name="dashInfo">
