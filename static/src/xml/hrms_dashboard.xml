            <div class="col-xs-12">
              <!-- top tiles -->
              <div class="row tile_count">
                <div class="col-sm-2  col-xs-6 tile_stats_count" id="expected-prem">
                  <span class="count_top">
                    <i class="fa fa-user"/>
                    Expected Premium
                  </span>
                  <div class="count" id="expected-prem-num">
                    <t t-esc="widget.makeNumber(widget.info['ExpectedPrem'][0])"/>
                    $
                  </div>
                  <span class="count_bottom">
                    <i class="green" id="expected-prem-prob">
                      <t t-esc="widget.makeNumber(widget.info['ExpectedPrem'][1])"/>
                      %
                    </i>
                    Average probability
                  </span>
                </div>
                <div class="col-sm-2 col-xs-6 tile_stats_count" id="premium-written">
                  <span class="count_top">
                    <i class="fa fa-clock-o"/>
