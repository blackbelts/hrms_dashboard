      <link href="/hrms_dashboard/static/src/css/jqvmap.min.css" rel="stylesheet" type="text/css"/>
      <!-- bootstrap-daterangepicker -->
      <link href="/hrms_dashboard/static/src/css/daterangepicker.css" rel="stylesheet" type="text/css"/>
